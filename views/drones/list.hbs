<h2>Available drones</h2>
{{#each droneList}}
 <div id="drone-div">
    <p>Name:</p>
  <h3 class="space-div">{{name}}</h3>
  <p>Proppellers:</p>
  <p class="space-div">{{propellers}}</p>
  <p>Max. Speed:</p>
  <p class="space-div">{{maxSpeed}}</p>
  <a class="space-div" href="/drones/{{_id}}/edit">Edit</a>
  <form class="space-div" action="/drones/{{_id}}/delete" method="post"><button>Delete</button></form>
 </div>
{{/each}}
